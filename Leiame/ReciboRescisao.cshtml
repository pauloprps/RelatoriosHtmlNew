@model Steps.ExtranetRH.Web.ViewModels.VisualizarReciboViewModel

@{ 
    Layout = null;
}
<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <title></title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="viewport" content="target-densitydpi=device-dpi, initial-scale=1.0, user-scalable=no" />
    @*<title>Dinamic Table | STEPS CONSULTORIA - FRONT TRCT</title>
    <link rel="icon" type="image/png" sizes="96x96" href="~/favicon.ico">*@
    <style>
        *{
            font-family: Arial, Helvetica, sans-serif;
        }
        body{
            background: #525659;
            margin:0;
            padding: 0;
        }

        /**********************************
        *   Botoes de acao no rodape
        ***********************************/
        a.btn {
            text-decoration: none;
        }

        .btn {
            display: inline-block;
            font-weight: 400;
            text-align: center;
            white-space: nowrap;
            vertical-align: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
            border: 1px solid transparent;
            padding: .375rem .75rem;
            font-size: 1rem;
            line-height: 1.5;
            border-radius: .25rem;
            transition: color .15s ease-in-out, background-color .15s ease-in-out, border-color .15s ease-in-out, box-shadow .15s ease-in-out
        }

            .btn:focus,
            .btn:hover {
                text-decoration: none
            }

            .btn.focus,
            .btn:focus {
                outline: 0;
                box-shadow: 0 0 0 .2rem rgba(0, 123, 255, .25)
            }

            .btn.disabled,
            .btn:disabled {
                opacity: .65
            }

            .btn:not(:disabled):not(.disabled) {
                cursor: pointer
            }

                .btn:not(:disabled):not(.disabled).active,
                .btn:not(:disabled):not(.disabled):active {
                    background-image: none
                }

        a.btn.disabled,
        fieldset:disabled a.btn {
            pointer-events: none
        }

        .btn-primary {
            color: #fff;
            background-color: #007bff;
            border-color: #007bff
        }

            .btn-primary.grey {
                color: rgb(7, 7, 7);
                background-color: #a5a5a5;
                border-color: #8b8b8b
            }

            .btn-primary:hover {
                color: #fff;
                background-color: #0069d9;
                border-color: #0062cc
            }

            .btn-primary.focus,
            .btn-primary:focus {
                box-shadow: 0 0 0 .2rem rgba(0, 123, 255, .5)
            }

            .btn-primary.disabled,
            .btn-primary:disabled {
                color: #fff;
                background-color: #007bff;
                border-color: #007bff
            }

            .btn-primary:not(:disabled):not(.disabled).active,
            .btn-primary:not(:disabled):not(.disabled):active,
            .show > .btn-primary.dropdown-toggle {
                color: #fff;
                background-color: #0062cc;
                border-color: #005cbf
            }

                .btn-primary:not(:disabled):not(.disabled).active:focus,
                .btn-primary:not(:disabled):not(.disabled):active:focus,
                .show > .btn-primary.dropdown-toggle:focus {
                    box-shadow: 0 0 0 .2rem rgba(0, 123, 255, .5)
                }

        .botoes {
            padding: 5px 0;
            position: fixed;
            bottom: 0;
            width: 100%;
            background: #393939;
        }

            .botoes .btn {
                font-size: 14px;
            }

        /**********************************
        *   END Botoes de acao no rodape
        ***********************************/

        .grey{
            background: #ddd;
        }
        .strong{
            font-weight: bold;
        }
        .border{
            border: 2px solid black;
        }
        .border{
            border-bottom: none;
        }
        #table_4 .border{
            border: 2px solid black;
        }
        .border.cel_1{
            border-left:0;
        }
        .content{
            background: #FFF;
            margin: 40px auto;
            padding: 5px;
            padding-bottom: 30px;
            box-shadow: 2px 2px 12px rgba(0,0,0,0.6);
            max-width: 820px;
            width: 100%;
        }
        .table{
            width: 100%;
            margin-bottom: -2px;
        }
        .table .table{
            width: calc(100% + 6px);
            margin-bottom: -3px;
            margin: 0 -3px -3px;
        }
        .table .table .border{
            border-bottom: 1px;
            border-right: 0;
        }
        .celula{
            height: 20px;
        }
        .super_celula{
            height: 30px;
        }
        .celula table td{
           padding: 0 5px;
        }
        .total table{
            width:100%;
        }
        .label{
            font-weight: bold!important;
        }
        .celula .label.id{
            width: 10%;
            min-width: 4px;
            display: block;
            padding-right: 0;
            letter-spacing: -0.5px;
            white-space: nowrap;
        }
        .celula .label.desc{
            width: 80%;
        }
        .total .label, .total .valor{
            text-align: center;
            font-size: 12px;
        }
        .celula .valor, .celula .valor_2{
            /* display: unset; */
        }

        .col_1, .col_3, .col_5{
            width:20%;
        }
        .col_2, .col_4, .col_6{
            width:10%;
        }
        .col_2 table, .col_4 table, .col_6 table{
            width: 100%;
        }
        .col_1 .valor, .col_3 .valor, .col_5 .valor{
            text-align: right;
            width: 5%;
            padding-right: 0;
        }
        .col_1 .valor_2, .col_3 .valor_2, .col_5 .valor_2 {
            text-align: left;
            width: 95%;
        }
        .col_2 .valor, .col_4 .valor, .col_6 .valor{
            width: 100%;
            text-align: center;
            display: table;
        }
        table, thead, tbody, th, tr, td{
            border-collapse: collapse;
        }
        th{
            font-size: 11px;
            font-weight: 300;
            padding: 3px;
        }
        td{
            font-size: 8px;
            font-weight: 300;
        }
        .center {
            text-align: center;
            justify-content: center;
            align-items: center;
        }
        #table_4 .celula{
            height: 20px;
        }
        @@media screen and (max-width: 796px){
            .content{
                margin: 40px 10px;
                max-width: initial;
                width: initial;
                display: inline-block;
            }
        }
        @@media print{
            .content{
                margin: 0;
                box-shadow: none;
                padding:0;
            }
            th h2{
                margin: 0;
            }

            .botoes {
                display: none;
            }

            .celula {
                height: 18px;
            }
            .super_celula{
                height: 20px;
            }
            #table_4 .celula{
               /* height: 6px;*/
            }
            .grey{
                box-shadow: 0 0 0 1000px #ddd inset;  /* cor para impressão */
            }
            .border, #table_4 .border{
                border: 1.5px solid black!important;
            }
            .table .table{
                width: calc(100% + 4px);
                margin-bottom: -3px;
                margin: 0 -2px -3px;
            }
        }

    </style>
</head>
<body>
    <div class="content">
        <table class="table border">
            <thead>
            <th class="grey strong ">
                <h2>TERMO DE RESCISÃO DO CONTRATO DE TRABALHO</h2>
            </th>
            </thead>
            <tbody>
                <tr>
                    <td>

                        <!-- Tabela 1 -->

                        <table id="table_1" class="table border">
                            <thead>
                            <th class="grey strong">
                                IDENTIFICAÇÃO DO EMPREGADOR
                            </th>
                            </thead>
                            <tbody>
                                <!-- Tabela 1 | LINHA 1 -->

                                <tr class="linha_1">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 cnpj_cei">
                                                <table>
                                                    <tr>
                                                        <td class="label">01 - CNPJ / CEI</td>
                                                        <td class="value">@Convert.ToInt64(Model.reciboRescisao.funcionario.Filial.Cnpj).ToString(@"00\.000\.000\/0000\-00")</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 razao_social_nome">
                                                <table>
                                                    <tr>
                                                        <td class="label">02 - Razão Social / Nome</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Empresa.Nome</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 1 | LINHA 2 -->

                                <tr class="linha_2">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 endereco">
                                                <table>
                                                    <tr>
                                                        <td class="label">03 - Endereço ( logradouro, nº, andar, apartamento )</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Filial.Logradouro , @Model.reciboRescisao.funcionario.Filial.Numero - @Model.reciboRescisao.funcionario.Filial.Complemento</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 bairro">
                                                <table>
                                                    <tr>
                                                        <td class="label">04 - Bairro</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Filial.Bairro</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 1 | LINHA 3 -->

                                <tr class="linha_3">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 municipio">
                                                <table>
                                                    <tr>
                                                        <td class="label">05 - Município</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Filial.Cidade</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 uf">
                                                <table>
                                                    <tr>
                                                        <td class="label">06 - UF</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Filial.UF</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_3 cep">
                                                <table>
                                                    <tr>
                                                        <td class="label">07 - CEP</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Filial.Cep</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_4 cnae">
                                                <table>
                                                    <tr>
                                                        <td class="label">08 - CNAE</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Filial.Cnae</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_5 cnpj_cei_tomador_obra">
                                                <table>
                                                    <tr>
                                                        <td class="label">09 - CNPJ / CEI Tomador / Obra</td>
                                                        <td class="value">@Convert.ToInt64(Model.reciboRescisao.funcionario.Cliente.CNPJ).ToString(@"00\.000\.000\/0000\-00")</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>

                <tr>
                    <td>

                        <!-- Tabela 2 -->

                        <table id="table_2" class="table border">
                            <thead>
                            <th class="grey strong">IDENTIFICAÇÃO DO TRABLHADOR</th>
                            </thead>
                            <tbody>
                                <!-- Tabela 2 | LINHA 1 -->

                                <tr class="linha_1">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 pis_pasep">
                                                <table>
                                                    <tr>
                                                        <td class="label">10 - PIS - PASEP</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.PIS</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 nome_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">11 - Nome</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Nome</td>
                                                        <td class="value value_2">@Model.reciboRescisao.funcionario.Codigo</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 2 | LINHA 2 -->

                                <tr class="linha_2">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 endereco_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">12 - Endereço ( logradouro, nº, andar, apartamento )</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Logradouro , @Model.reciboRescisao.funcionario.Numero - @Model.reciboRescisao.funcionario.Complemento</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 bairro_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">13 - Bairro</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Bairro</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 2 | LINHA 3 -->

                                <tr class="linha_3">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 municipio_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">14 - Município</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Cidade</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 uf_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">15 - UF</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.UF</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_3 cep_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">16 - CEP</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Cep</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_4 carteira_trabalho">
                                                <table>
                                                    <tr>
                                                        <td class="label">17 - Carteira de Trabalho (nº, sèrie, UF)</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.NumeroCtps @Model.reciboRescisao.funcionario.SerieCtps @Model.reciboRescisao.funcionario.UfCtps</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_5 cpf_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">18 - CPF</td>
                                                        <td class="value">@Convert.ToInt64(Model.reciboRescisao.funcionario.CPF).ToString(@"000\.000\.000\-00")</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 2 | LINHA 4 -->

                                <tr class="linha_4">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 data_nascimento_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">19 - Data de Nascimento</td>
                                                        <td class="value">@(Model.reciboRescisao.funcionario.DataNascimento.HasValue ? Convert.ToDateTime(Model.reciboRescisao.funcionario.DataNascimento).ToString("dd/MM/yyyy") : "")</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 nome_mae_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">20 - Nome da Mãe</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.NomeMae</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                            </tbody>
                        </table>

                    </td>
                </tr>

                <tr>
                    <td>

                        <!-- Tabela 3 -->

                        <table id="table_3" class="table border">
                            <thead>
                            <th class="grey strong ">DADOS DO CONTRATO</th>
                            </thead>
                            <tbody>
                                <!-- Tabela 3 | LINHA 1 -->

                                <tr class="linha_1">
                                    <td>
                                        <table class="table">

                                            <td class="border celula cel_1 tipo_contrato">
                                                <table>
                                                    <tr>
                                                        <td class="label">21 - Tipo de Contrato</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.Vinculo</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 causa_afastamento">
                                                <table>
                                                    <tr>
                                                        <td class="label">22 - Causa do Afastamento</td>
                                                        <td class="value">@Model.reciboRescisao.tabelaRescisao.CausaRescisao</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 3 | LINHA 2 -->

                                <tr class="linha_2">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 remuneracao_mes_anterior">
                                                <table>
                                                    <tr>
                                                        <td class="label">23 - Remuneração Mês Anterior R$</td>
                                                        <td class="value">@String.Format("{0:N}", Convert.ToDecimal(Model.reciboRescisao.Calculo.Salario))</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 data_admissao">
                                                <table>
                                                    <tr>
                                                        <td class="label">24 - Data de Admissão</td>
                                                        <td class="value">@(Model.reciboRescisao.funcionario.DataAdmissao.HasValue ? Convert.ToDateTime(Model.reciboRescisao.funcionario.DataAdmissao).ToString("dd/MM/yyyy") : "")</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_3 data_aviso_previo">
                                                <table>
                                                    <tr>
                                                        <td class="label">25 - Data do Aviso Prévio</td>
                                                        <td class="value">@(Model.reciboRescisao.funcionario.DataAvisoPrevio.HasValue ? Convert.ToDateTime(Model.reciboRescisao.funcionario.DataAvisoPrevio).ToString("dd/MM/yyyy") : "")</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_4 data_afastamento">
                                                <table>
                                                    <tr>
                                                        <td class="label">26 - Data de Afastamento</td>
                                                        <td class="value">@(Model.reciboRescisao.funcionario.DataDemissao.HasValue ? Convert.ToDateTime(Model.reciboRescisao.funcionario.DataDemissao).ToString("dd/MM/yyyy") : "")</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 3 | LINHA 3 -->

                                <tr class="linha_3">
                                    <td>
                                        <table class="table">
                                            <td class="border celula cel_1 cod_afastamento">
                                                <table>
                                                    <tr>
                                                        <td class="label">27 - Cód. Afastamento</td>
                                                        <td class="value">@Model.reciboRescisao.tabelaRescisao.CausaRescisao.Substring(0, 3)</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 pensao_alimenticia_trct">
                                                <table>
                                                    <tr>
                                                        <td class="label">28 - Pensão Alimentícia (%) (TRCT)</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.PorcentagemPensao</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_3 pensao_alimenticia_fgts">
                                                <table>
                                                    <tr>
                                                        <td class="label">29 - Pensão Alimentícia (%) (FGTS)</td>
                                                        <td class="value">@Model.reciboRescisao.funcionario.PorcentagemPensao</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_4 categoria_trabalhador">
                                                <table>
                                                    <tr>
                                                        <td class="label">30 - Categoria do Trabalhador</td>
                                                        @switch (Model.reciboRescisao.funcionario.Vinculo)
                                                        {
                                                            case 11:
                                                                <td class="value">04</td>
                                                                break;
                                                            case 17:
                                                                <td class="value">06</td>
                                                                break;
                                                            case 14:
                                                                <td class="value">06</td>
                                                                break;
                                                            default:
                                                                <td class="value">01</td>
                                                                break;
                                                        }
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>

                                <!-- Tabela 3 | LINHA 4 -->

                                <tr class="linha_4">
                                    <td>
                                        <table class="table">

                                            <td class="border celula cel_1 cod_sindical">
                                                <table>
                                                    <tr>
                                                        <td class="label">31 - Código Sindical</td>
                                                        <td class="value">@Model.reciboRescisao.sindicato.CodigoOificial</td>
                                                    </tr>
                                                </table>
                                            </td>

                                            <td class="border celula cel_2 cnpj_nome_ent_sind_lab">
                                                <table>
                                                    <tr>
                                                        <td class="label">32 - CNPJ e Nome da Entidade Sindical Laboral</td>
                                                        <td class="value">@Convert.ToInt64(Model.reciboRescisao.sindicato.CNPJ).ToString(@"00\.000\.000\/0000\-00") @Model.reciboRescisao.sindicato.Nome</td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </table>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                    </td>
                </tr>

                <tr>
                    <td>

                        <!-- Tabela 4 -->

                        <table id="table_4" class="table">
                            <thead>
                                <tr>
                                    <th colspan="6" class="grey strong border">DISCRIMINAÇÃO DAS VERBAS RESCISÓRIAS</th>
                                </tr>
                                <tr>
                                    <th colspan="6" class="border">VERBAS RESCISÓRIAS</th>
                                </tr>
                                <tr>
                                    <th class="border" align="left">Rúbrica</th>
                                    <th class="border">Valor</th>
                                    <th class="border" align="left">Rúbrica</th>
                                    <th class="border">Valor</th>
                                    <th class="border" align="left">Rúbrica</th>
                                    <th class="border">Valor</th>
                                </tr>

                            </thead>
                            <tbody id="rubricas">

